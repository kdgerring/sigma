    (instance ?VIDEO Camera)
    (located ?DISPLAY ?LOC2)
    (not (equal ?LOC1 ?LOC2))
    (instance ?DISPLAY VideoDisplay))
  (hasPurpose ?X
    (exists (?PROC ?VID ?RL ?CAPTURE)
      (and
        (instance ?PROC Process)
        (located ?PROC ?LOC1)
        (represents ?VID ?PROC)
        (instrument ?CAPTURE Camera)
        (result ?CAPTURE ?VID)
        (instance ?RL RadiatingLight)
        (instrument ?RL ?DISPLAY)
        (patient ?RL ?VID)
        (before
          (BeginFn ?PROC)
