      (customer ?CUST ?AGENT)
      (serviceProvider ?CHECK ?AGENT)
      (instance ?GREET Greeting)
      (agent ?GREET ?CUST)
      (destination ?GREET ?AGENT)
      (subProcess ?GREET ?CHECK)
      (instance ?SERVICE Process)
      (agent ?SERVICE ?AGENT)
      (causes ?CHECK ?SERVICE))))

