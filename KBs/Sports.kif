  (or
    (exists (?W)
      (and
        (instance ?W WinterSeason)
        (temporalPart (WhenFn ?O) ?W)))
    (exists (?S)
      (and
        (instance ?S SummerSeason)
        (temporalPart (WhenFn ?O) ?S)))))
        
