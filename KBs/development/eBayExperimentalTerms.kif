;; RegisteredUser
;; -------------------------------------------------------------------

(subclass RegisteredUser Attribute)

(documentation RegisteredUser EnglishLanguage "A user registered in the &%EbayUserDatabase")

(termFormat EnglishLanguage RegisteredUser "registered user")

(=>
  (attribute ?USER RegisteredUser)
  (exists (?PART)
    (and
      (instance ?USER EbayVisitor)
      (part ?PART EbayUserDatabase)
      (represents ?PART ?USER))))

;; EbayPurchases
;; -------------------------------------------------------------------

(subclass EbayPurchases Collection)

(documentation EbayPurchases EnglishLanguage "The &%collection of all instances of &%Buying by a 
 &%Human on the &%EbayWebsite. the buyer possesses this collection.")

(termFormat EnglishLanguage EbayPurchases "ebay purchases")
    

(=>
  (and
    (instance ?PURCHASES EbayPurchases)
    (instance ?BUYER Human)
    (possesses ?BUYER ?PURCHASES)
    (instance ?BUYING Buying)
    (agent ?BUYING ?BUYER)
    (instrument ?BUYING EbayWebsite))
  (member ?BUYING ?PURCHASES))


;; EbayQualifyingPurchasesFn
;; -------------------------------------------------------------------

(documentation EbayQualifyingPurchasesFn EnglishLanguage "(EbayQualifyingPurchasesFn ?HUMAN 
 ?EXPERIMENT ?INTERVAL) returns the &%collection of all instances of &%Buying in ?INTERVAL by a
 &%Human who is a on the &%EbayWebsite, and where during the &%AccessingWebPage the ?HUMAN
 experienced a &%process which is either the &%experiementalVariableProccess or 
 &%experiementalControlProccess in ?EXPERIMENT before the &%Buying.")

(instance EbayQualifyingPurchasesFn BinaryFunction)

(domain EbayQualifyingPurchasesFn 1 Experimenting)

(domain EbayQualifyingPurchasesFn 2 TimeInterval)

(range Class)

(termFormat EnglishLanguage EbayQualifyingPurchases "eBay qualifying purchases")

(=>
  (and
    (instance ?EXPERIMENT Experimenting)
    (instance ?INTERVAL TimeInterval))
  (equals 
    (EbayQualifyingPurchases ?EXPERIMENT ?INTERVAL)
    (KappFn ?BUYING
      (and
        (instance ?VISITOR EbayVisitor)
        (instance ?BUYING Buying)
        (instance ?ACCESSING AccessingWebPage)
        (instance ?EVENT Proccess)
        (member ?EVENT (QualifyingEventsFn ?EXPERIMENT)
        (agent ?BUYING ?VISITOR)
        (agent ?ACCESSING ?VISITOR)
        (during (WhenFn ?BUYING) (WhenFn ?ACCESSING))
        (during (WhenFn ?EVENT) (WhenFn ?ACCESSING))
        (before (BeginFn ?EVENT) (BeginFn ?BUYING))
        (during (WhenFn ?ACCESSING) ?INTERVAL)
        (destination ?ACCESSING ?PAGE)
        (ebayTreatedPage ?PAGE ?EXPERIMENT))))))

;; EbaySales
;; -------------------------------------------------------------------

(subclass EbaySales Collection)

(documentation EbaySales EnglishLanguage "The &%collection of all instances of &%Buying sold by a 
 &%Human on the &%EbayWebsite. The Seller &%possesses this collection")

(termFormat EnglishLanguage EbaySales "eBay sales")
    

(=>
  (and
    (instance ?SALES EbaySales)
    (instance ?SELLER Human)
    (possesses ?SELLER ?SALES)
    (instance ?BUYING Buying)
    (origin ?BUYING ?SELLER)
    (instrument ?BUYING EbayWebsite))
  (member ?BUYING ?SALES))

;; JW: Better caserole for than instrument?

;; experiementalVariableProccess
;; -------------------------------------------------------------------

(documentation experiementalVariableProccess EnglishLanguage "(experiementalVariableProccess 
 ?EXPERIMENT ?PROC) means that the &%experiencing of the &%Process ?PROC is the experimental 
 independent variable in the instance of experimenting ?EXPERIMENT, i.e. ?PROC is what is different 
 from the &%experimentalControlProcess &%experienced by the control group in the experiment")

(domain experiementalVariableProccess 1 Experimenting)

(domain experiementalVariableProccess 2 Process) 

(instance experiementalVariableProccess CaseRole) 

(subrelation experiementalVariableProccess patient)

(termFormat EnglishLanguage experimentalVariableProcess "experimental variable (process)")



;; experimentalControlProccess
;; -------------------------------------------------------------------

(documentation experimentalControlProccess EnglishLanguage "(experimentalControlProccess ?EXPERIMENT
 ?PROC) means that the &%experiencing of the &%Process ?PROC is the control in the instance of 
 experimenting ?EXPERIMENT, i.e. ?PROC is what is different from an &%experimentalVariableProcess 
 &%experienced by the experiemental group in the experiment")

(domain experimentalControlProccess 1 Experimenting)

(domain experimentalControlProccess 2 Process) 

(instance experimentalControlProccess CaseRole) 

(subrelation experimentalControlProccess patient)

(termFormat EnglishLanguage experimentalControlProcess "experimental control (process)")

