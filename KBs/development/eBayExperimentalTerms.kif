;; EbayVisitor (Visitor)
;; -------------------------------------------------------------------

(subclass EbayVisitor Human)

(documentation EbayVisitor EnglishLanguage "Someone who visits the eBay site, who may or may not log
 into the site.")

(termFormat EnglishLanguage EbayVisitor "eBay Visitor")

(synonymousExternalConcept "visitor" EbayVisitor EbayAcronyms)

(=>
  (instance ?VISITOR EbayVisitor)
  (exists (?WEBPAGE ?ACCESSING)
    (and
      (instance ?WEBPAGE WebPage)
      (component ?WEBPAGE EbayWebSite)
      (instance ?ACCESSING AccessingWebPage)
      (agent ?ACCESSING ?VISITOR)
      (destination ?ACCESSING ?WEBPAGE))))
    

;; EbayGUID (GUID)
;; -------------------------------------------------------------------

(subclass EbayGUID Identifier)

(documentation EbayGUID EnglishLanguage "Global user identifier, machine identifier based on browser
 cookies.")

(termFormat EnglishLanguage EbayGUID "eBay GUID")

(synonymousExternalConcept "GUID" EbayGUID EbayAcronyms)

(=>
  (instance ?GUID EbayGUID)
  (exists (?COMPUTER ?WEBPAGE ?ACCESSING)
    (and
      (instance ?WEBPAGE WebPage)
      (component ?WEBPAGE EbayWebSite)
      (instance ?ACCESSING AccessingWebPage)
      (instrument ?ACCESSING ?COMPUTER)
      (destination ?ACCESSING ?WEBPAGE)
      (represents ?GUID ?COMPUTER))))


;; EbayUID (UID)
;; -------------------------------------------------------------------

(subclass EbayUID Identifier)

(documentation EbayUID EnglishLanguage "User identifier, established when the user registers on
 eBay.com")

(termFormat EnglishLanguage EbayUID "eBay UID")

(synonymousExternalConcept "UID" EbayUID EbayAcronyms)

(=>
  (instance ?UID EbayUID)
  (exists (?USER)
    (and
      (instance ?USER EbayVisitor)
      (attribute ?USER RegisteredUser)
      (represents ?UID ?USER))))

;; EbayGMBFn (GMB)
;; -------------------------------------------------------------------

(subclass EbayGMBFn BinaryFn)

(domain EbayGMBFn 1 Human)

(domain EbayGMBFn 2 TimeInterval)


(range EbayGMBFn CurrencyMeasure)

(documentation EbayGMBFn EnglishLanguage "The total value of goods (in Local Currency or US dollars)
 bought by a buyer, based on The buyer country.")

(termFormat EnglishLanguage EbayGMBFn "eBay GMB")

(synonymousExternalConcept "GMB" EbayGMBFn EbayAcronyms)

(=>
  (and
    (instance ?PURCHASES EbayPurchases)
    (instance ?INTERVAL TimeInterval)
    (instance ?USER Human)
    (possesses ?USER ?PURCHASES))
  (and
    (monetaryValue (KappaFn ?AMOUNT
        (and
          (member ?PURCHASE ?PURCHASES)
          (during (WhenFn ?PURCHASE) ?INTERVAL)
          (transactionAmount ?PURCHASE ?AMOUNT)))
        ?TOTAL_PURCHASE_AMOUNT)
    (equal 
      (EbayGMBFn ?USER ?INTERVAL) (?TOTAL_PURCHASE_AMOUNT))))

;; does this work? i.e. adds up all puchase amounts?

;; EbayGMVFn (GMV)
;; -------------------------------------------------------------------

(subclass EbayGMVFn BinaryFn)

(domain EbayGMVFn 1 Human)

(domain EbayGMVFn 2 TimeInterval)

(range EbayGMVFn CurrencyMeasure)

(documentation EbayGMVFn EnglishLanguage "Total amount of all purchases made  (in local currency or 
 US dollars), reported by seller country of residence (Item Price * Quantity Sold).")

(termFormat EnglishLanguage EbayGMVFn "eBay GMV")

(synonymousExternalConcept "GMV" EbayGMVFn EbayAcronyms)


(=>
  (and
    (instance ?SALES EbaySales)
    (instance ?INTERVAL TimeInterval)
    (instance ?USER Human)
    (possesses ?USER ?SALES)
    (monetaryValue (KappaFn ?AMOUNT
      (and
        (member ?SALE ?SALES)
        (during (WhenFn ?SALE) ?INTERVAL)
        (transactionAmount ?SALE ?AMOUNT)))
      ?TOTAL_SALES_AMOUNT))
  (equal 
    (EbayGMBFn ?USER ?INTERVAL) (?TOTAL_SALES_AMOUNT)))


;; EbayLTU (LTU)
;; -------------------------------------------------------------------

(subclass EbayLTU Document)

(documentation EbayLTU EnglishLanguage "Live Test Update, which has come to mean the final report of
 test results.")

(termFormat EnglishLanguage EbayLTU "eBay LTU")

(synonymousExternalConcept "LTU" EbayLTU EbayAcronyms)

(=>
  (instance ?LTU EbayLTU)
  (exists (?EXPERIMENTING ?RESULT)
    (and
      (instance ?EXPERIMENTING Experimenting)
      (result ?EXPERIMENTING ?RESULT)
      (containsInformation ?LTU ?RESULT))))

;; EbayLift (lift)
;; -------------------------------------------------------------------

;; EbayPPWFn (PPW)
;; -------------------------------------------------------------------

(domain EbayPPWFn 1 Week)

(range EbayPPWFn RealNumber)

(documentation EbayPPWFn EnglishLanguage "Purchases per visitor per week")

(termFormat EnglishLanguage EbayPPWFn "eBay PPW")

(synonymousExternalConcept "PPW" EbayPPWFn EbayAcronyms)

(=>
  (and
    (instance ?WEEK Week)
    (instance ?PURCHASES Collection)
    (instance ?VISITORS Collection)
    (forall (?VISITOR ?ACCESSING ?WEBPAGE)
      (=>
        (and
          (instance ?VISITOR EbayVisitor)
          (instance ?WEBPAGE WebPage)
          (component ?WEBPAGE EbayWebSite)
          (instance ?ACCESSING AccessingWebPage)
          (agent ?ACCESSING ?VISITOR)
          (destination ?ACCESSING ?WEBPAGE)
          (during (WhenFn ?ACCESSING) ?WEEK))
        (member ?VISITOR ?VISITORS)))
    (forall (?PURCHASE ?BUYING)
      (=>
        (and
          (instance ?BUYING Buying)
          (instrument ?BUYING EbayWebsite)
          (during (WhenFn ?BUYING) ?WEEK))
        (member ?PURCHASE ?PURCHASES))))
  (equal 
    (EbayPPWFn ?WEEK)
    (DivisionFn
      (CardinalityFn ?PURCHASES)
      (CardinalityFn ?VISITORS))))


;; EbayGPWFn (GPW)
;; -------------------------------------------------------------------

(domain EbayGPWFn 1 Week)

(documentation EbayGPWFn EnglishLanguage "GMB per visitor per week.")

(termFormat EnglishLanguage EbayGPWFn "eBay GPW")

(synonymousExternalConcept "GPW" EbayGPWFn EbayAcronyms)

(=>
  (and
    (instance ?WEEK Week)
    (instance ?VISITORS_IN_WEEK Collection)
    (instance ?GMBS_IN_WEEK Collection)
    (forall (?VISITOR ?PURCHASE ?WEBPAGE ?ACCESSING)
      (=>
        (and
          (instance ?VISITOR EbayVisitor)
          (instance ?WEBPAGE WebPage)
          (component ?WEBPAGE EbayWebSite)
          (instance ?ACCESSING AccessingWebPage)
          (agent ?ACCESSING ?VISITOR)
          (destination ?ACCESSING ?WEBPAGE)
          (during (WhenFn ?ACCESSING) ?WEEK))
        (and
          (member (EbayGMBFn ?VISITOR ?GMBS_IN_WEEK))
          (member ?VISITOR ?VISITORS_IN_WEEK))))
    (monetaryValue ?GMBS_IN_WEEK ?TOTAL_GMB))
  (equal 
    (EbayGPWFn ?WEEK)
    (DivisionFn
      (?TOTAL_GMB)
      (CardinalityFn ?VISITORS_IN_WEEK))))


;; EbayQualifyingEventsFn (Qualifying Factor/Event)
;; -------------------------------------------------------------------

(documentation EbayQualifyingEventsFn EnglishLanguage "A &%UnaryFunction that maps an instance of
 &%Experimenting to the class containing all &%experimentalControlProcceses and
 &%experiementalVariableProcceses")
    

(subclass EbayQualifyingEventsFn UnaryFunction)

(domain EbayQualifyingEventsFn 1 Experimenting)

(range EbayQualifyingEventsFn Class)

(termFormat EnglishLanguage EbayQualifyingEventsFn "eBay Qualifying Factor or Event")

(synonymousExternalConcept "Qualifying Factor/Event" EbayQualifyingEventsFn EbayAcronyms)

(=>
  (instance ?EXPERIMENT Experimenting)
  (equal 
    (EbayQualifyingEventsFn ?EXPERIMENT)
    (KappaFn ?EVENT
      (or
        (experimentalControlProccess ?EXPERIMENT ?EVENT )
        (experiementalVariableProccess ?EXPERIMENT ?EVENT)))))

;; ebayTreatedPage
;; -------------------------------------------------------------------

(domain ebayTreatedPage 1 WebPage)

(domain ebayTreatedPage 2 Experimenting)

(termFormat EnglishLanguage ebayTreatedPage "eBay treated page")

(instance ebayTreatedPage BinaryPredicate)

(documentation ebayTreatedPage EnglishLanguage "Pages on the site where the qualified event/factor 
surface.")

(synonymousExternalConcept "Treated Page" ebayTreatedPage EbayAcronyms)

(=>
  (ebayTreatedPage ?WEBPAGE ?EXPERIMENT)
  (exists (?EVENT)
    (and
      (member ?EVENT (EbayQualifyingEventsFn ?EXPERIMENT))
      (=>
        (exists (?VISITOR ?ACCESSING)
          (and
            (instance ?VISITOR EbayVisitor)
            (instance ?ACCESSING AccessingWebPage)
            (agent ?ACCESSING ?VISITOR)
            (destination ?ACCESSING ?WEBPAGE)))
        (experiencer ?VISITOR ?EVENT)))))

;; ebayTreatedUser
;; -------------------------------------------------------------------

(domain ebayTreatedUser 1 EbayVisitor)

(domain ebayTreatedUser 2 Experimenting)

(domain ebayTreatedUser 3 Interval)

(synonymousExternalConcept "Treated User" ebayTreatedUser EbayAcronyms)

(documentation ebayTreatedUser EnglishLanguage "A GUID/UID qualified for the experiment.")

(instance ebayTreatedUser TernaryPredicate)

(=>
  (ebayTreatedUser ?VISITOR ?EXPERIMENT ?INTERVAL)
  (exists (?VISITOR ?EVENT)
    (and
      (instance ?VISITOR EbayVisitor)
      (member ?EVENT (EbayQualifyingEventsFn ?EXPERIMENT))
      (during (WhenFn ?EVENT) ?INTERVAL)
      (experiencer ?VISITOR ?EVENT))))

;; ebayTreatedUsersFn
;; -------------------------------------------------------------------

(domain EbayTreatedUsersFn 1 Experimenting)

(domain EbayTreatedUsersFn 2 Interval)

(range EbayTreatedUsersFn Collection)

(synonymousExternalConcept "Treated Users" ebayTreatedUser EbayAcronyms)

(instance ebayTreatedUsersFn BinaryFunction)


(=>
  (and
    (instance ?EXPERIMENT Experiment)
    (instance ?INTERVAL TimeInterval))
  (equal
    (EbayTreatedUsersFn ?EXPERIMENT ?INTERVAL)
    (KappaFn ?USER
      (and
        (instance ?USER EbayVisitor)
        (treatedUser ?USER ?EXPERIMENT ?INTERVAL)))))


;; EbayPPSFn (PPS)
;; -------------------------------------------------------------------

(domain EbayPPSFn 1 Experimenting)

(domain EbayPPSFn 2 Week)

(documentation EbayPPSFn EnglishLanguage "Session initiated purchases per visitor per week. Accounts
 for purchases immediately following a qualifying event")

(termFormat EnglishLanguage EbayPPSFn "eBay PPS")

(synonymousExternalConcept "PPS" EbayPPSFn EbayAcronyms)



(equal
  (DivisionFn
    (CardinalityFn (EbayQualifyingPurchases ?EXPERIMENT ?WEEK))
    (CardinalityFn (EbayTreatedUserFn ?EXPERIMENT ?WEEK)))
  (EbayPPSFn ?EXPERIMENT ?WEEK))




;; EbayGPSFn
;; -------------------------------------------------------------------

(domain EbayGPSFn 1 Experimenting)

(domain EbayGPSFn 2 TimeInterval)

(documentation EbayGPSFn EnglishLanguage "GMB value for PPS purchases (item Price * Quantitiy 
Sold)")

(termFormat EnglishLanguage EbayGPSFn "eBay PPS")

(synonymousExternalConcept "PPS" EbayGPSFn EbayAcronyms)

(=>
  (and
    (instance ?EXPERIMENT Experimenting)
    (instance ?INTERVAL TimeInterval))
  (equal 
    (DivisionFn
      (monetaryValue (KappaFn ?AMOUNT
        (and
          (member (EbayQualifyingPurchases ?EXPERIMENT ?INTERVAL))
          (transactionAmount ?SALE ?AMOUNT))))
      (CardinalityFn (EbayTreatedUserFn ?EXPERIMENT ?INTERVAL)))
    (EbayGPSFn ?EXPERIMENT ?INTERVAL)))

;; EbayQuery (Query)
;; -------------------------------------------------------------------

(subclass Query SymbolicString)

(documentation EbayQuery EnglishLanguage "A search query is a query that a user enters into web 
 search engine to satisfy his or her information needs")

(termFormat EnglishLanguage EbayQuery "query")

(synonymousExternalConcept "Query" EbayGPSFn EbayAcronyms)


(=>
  (instance ?QUERY Query)
  (and
    (instance ?QUESTION Questioning)
    (result ?QUESTION ?QUERY)
    (patient ?QUESTION ?SEARCHENGINE)
    (instance ?SEARCHENGINE SearchEngine)))

;; EbayPandA (P&A)
;; -------------------------------------------------------------------

(subclass EbayPandA Class)

(documentation EbayPandA EnglishLanguage "Parts and Accessories category")

(termFormat EnglishLanguage EbayPandA "P&A")

(synonymousExternalConcept "P&A" EbayPandA EbayAcronyms)

(=>
  (instance ?ITEM EBayPandA)
  (hasPurpose ?ITEM
    (exists (?VEHICLE)
      (and
        (instance ?VEHICLE Vehicle)
        (or
          (part ?ITEM ?VEHICLE)
          (references ?ITEM ?VEHICLE))))))


;; EbayPandAItem (P&A Item)
;; -------------------------------------------------------------------

(subclass EbayPandAItem Object)

(documentation EbayPandAItem EnglishLanguage "An item listed under the P&A category or 
sub-categories")

(termFormat EnglishLanguage EbayPandAItem "P&A Item")

(synonymousExternalConcept "P&A Item" EbayPandAItem EbayAcronyms)

(=>
 (instance ?ITEM EbayPandAItem)
 (and
  (instance ?ITEM EbayPandA)
  (capability 
    (KappaFn ?SELLING
      (and
        (instance ?SELLING Selling)
        (instrument ?SELLING EbayWebsite)))
    patient ?ITEM)))

;; SearchResultsPage (SRP)
;; -------------------------------------------------------------------

(subclass SearchResultsPage WebPage)

(documentation SearchResultsPage EnglishLanguage "search results page, the list of results returned 
by a search engine in response to a user query")

(termFormat EnglishLanguage SearchResultsPage "SRP")

(synonymousExternalConcept "SRP" SearchResultsPage EbayAcronyms)

(=>
  (instance ?PAGE SearchResultsPage)
  (exists (?QUERY)
    (and
      (instance ?QUERY Query)
      (result ?QUESTION ?QUERY)
      (instance ?QUESTION Questioning)
      (agent ?ANSWER ?SEARCHENGINE)
      (instance ?ANSWER Answering)
      (instance ?SEARCHENGINE SearchEngine)
      (result ?ANSWER ?PAGE))))

;; WebPageModule (Module)
;; -------------------------------------------------------------------

(subclass WebPageModule UIElement)

(documentation WebPageModule EnglishLanguage "this is a component served on a web page eg. Search 
results, ad, etc.")

(termFormat EnglishLanguage WebPageModule "Module")

(synonymousExternalConcept "Module" WebPageModule EbayAcronyms)

(=>
  (instance ?MODULE WebPageModule)
  (exists (?PAGE)
    (and
      (instance ?PAGE WebPage)
      (part ?MODULE ?PAGE))))

;; EbayTPS (TPS)
;; -------------------------------------------------------------------

(subclass EbayTPS Document)

(documentation EbayTPS EnglishLanguage "Test Proposal Specification.")

(termFormat EnglishLanguage EbayTPS "eBay TPS")

(synonymousExternalConcept "TPS" EbayTPS EbayAcronyms)

(=>
  (instance ?TPS EbayTPS)
  (exists (?PLAN ?EXPERIMENT ?PROC)
    (and
      (instance ?PLAN Plan)
      (instance ?EXPERIMENT Experiment)
      (represents ?PLAN ?PROC)
      (subprocess ?EXPERIMENT ?PROC)
      (containsInformation ?TPS ?PLAN))))


;; EbayCBT (CBT)
;; -------------------------------------------------------------------

(subclass EbayCBT Attibute)

(documentation EbayCBT EnglishLanguage "Cross Border Trading. An item is CBT item where buyer 
registered country and seller registered country are not the same")

(termFormat EnglishLanguage EbayCBT "CBT Item")

(synonymousExternalConcept "CBT" EbayCBT EbayAcronyms)

(=>
  (attribute ?OBJ EbayCBT)
  (instance ?OBJ Object))

(=>
  (and
    (attribute ?OBJ EbayCBT)
    (instance ?OBJ Object))
  (exists (?SELLING ?SELLER ?BUYER)
    (and
      (instance ?SELLING Selling)
      (patient ?SELLING ?OBJ)
      (instance ?BUYER Human)
      (instance ?SELLER Human)
      (destination ?SELLING ?BUYER)
      (agent ?SELLING ?SELLER))))

(=>
  (and
    (attribute ?OBJ EbayCBT)
    (instance ?OBJ Object)
    (instance ?SELLING Selling)
    (patient ?SELLING ?OBJ)
    (destination ?SELLING ?BUYER)
    (agent ?SELLING ?SELLER))
  (exists (?BUYER_LOCATION ?SELLER_LOCATION)
    (and
      (instance ?BUYER_LOCATION GeoPoliticalArea)
      (instance ?SELLER_LOCATION GeoPoliticalArea)
      (located ?SELLER ?SELLER_LOCATION)
      (located ?BUYER ?BUYER_LOCATION)
      (not
        (equal ?BUYER_LOCATION ?SELLER_LOCATION)))))

    