;; RegisteredUser
;; -------------------------------------------------------------------

(subclass RegisteredUser Attribute)

(documentation RegisteredUser EnglishLanguage "A user registered in the &%EbayUserDatabase")

(termFormat EnglishLanguage RegisteredUser "registered user")

(=>
