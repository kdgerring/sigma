;; BBOWAFn (BBOWA)
;; -------------------------------------------------------------------

(domain BBOWAFn 1 EbayVisitor)

(range BBOWAFn Collection)

(termFormat EnglishLanguage BBOWAFn "BBOWA")

(synonymousExternalConcept "BBOWA" BBOWAFn EbayAcronyms)

(documentation BBOWAfn EnglishLanguage "Acronym for Bids, BINs, Orders, Watches, ASQ's. Represents
 user intent to purchase an item")
    

(=>
  (instance ?AGENT EbayVisitor)
  (equal
    (KappaFn (?PROCESSES)
      (and
        (agent ?PROCESS ?AGENT)
        (or
          (instance ?PROCESS EbayWatchItem)
          (instance ?PROCESS EbayASQ)
          (and
            (instance ?PROCESS Buying)
            (instrument ?PROCESS EbayWebSite))
          (and
            (instance ?PROCESS Requesting)
            (instrument ?PROCESS EbayWebSite)))))
    (BBOWAFn ?AGENT)))


