;; LiftFn (lift)
;; -------------------------------------------------------------------

(instance LiftFn BinaryFunction)

(domain LiftFn 1 Quantity)

(domain LiftFn 2 Quantity)

(range LiftFn RealNumber)

(termFormat EnglishLanguage LiftFn "lift")

(synonymousExternalConcept "Lift" LiftFn EbayAcronyms)

(documentation LiftFn EnglishLanguage "(LiftFn ?QUANTITY1 ?QUANTITY2) returns the lift of the
 quantities. i.e. (?QUANTITY1 - ?QUANTITY2)/(?QUANTITY2). In an experiment ?QUANTITY2 is the 
 &%Quantity that represents the control group.")

(=>
  (and
    (instance ?QUANTITY1 Quantity)
    (instance ?QUANTITY2 Quantity))
  (equal 
    (LiftFn ?QUANTITY1 ?QUANTITY2)
    (divisionFn
      (SubtractionFn ?QUANTITY1 ?QUANTITY2)
      ?QUANTITY2)))


;; PPWFn (PPW)
;; -------------------------------------------------------------------

(instance PPWFn BinaryFunction)

(domain PPWFn 1 Week)

(domain PPWFn 2 Collection)

(range PPWFn RealNumber)

(documentation PPWFn EnglishLanguage "Purchases per visitor per week. NEW: (PPWFn ?WEEK ?PAGES)
 returns the &%RealNumber that represents the number of instances of &%Buying in which a ?PAGE in 
 the collection ?PAGES is the instrument of &%Buying, divided by the number of visitors to pages in
 that collection, during a given ?WEEK)"

(termFormat EnglishLanguage PPWFn "eBay PPW")

(synonymousExternalConcept "PPW" PPWFn EbayAcronyms)

(=>
  (and
    (instance ?WEEK Week)
    (instance ?PURCHASES Collection)
    (instance ?VISITORS Collection)
    (instance ?PAGES Collection)
    (forall (?PAGE)
      (=>
        (member ?PAGE ?PAGES)
        (instance ?PAGE WebPage)))
    (forall (?VISITOR ?ACCESSING)
      (<=>
        (and
          (instance ?VISITOR EbayVisitor)
          (member ?PAGE ?PAGES)
          (instance ?ACCESSING AccessingWebPage)
          (agent ?ACCESSING ?VISITOR)
          (destination ?ACCESSING ?PAGE)
          (during (WhenFn ?ACCESSING) ?WEEK))
        (member ?VISITOR ?VISITORS)))
    (forall (?PURCHASE ?BUYING)
      (<=>
        (and
          (instance ?BUYING Buying)
          (member ?PAGE ?PAGES)
          (instrument ?BUYING ?PAGE)
          (during (WhenFn ?BUYING) ?WEEK))
        (member ?PURCHASE ?PURCHASES))))
  (equal 
    (PPWFn ?WEEK ?PAGES)
    (DivisionFn
      (CardinalityFn ?PURCHASES)
      (CardinalityFn ?VISITORS))))


;; GPWFn (GPW)
;; -------------------------------------------------------------------

(domain GPWFn 1 Week)

(domain GPWFn 2 Collection)

(range GPWFn CurrencyMeasure)

(documentation GPWFn EnglishLanguage "GMB per visitor per week.")

(termFormat EnglishLanguage GPWFn "GPW")

(synonymousExternalConcept "GPW" GPWFn EbayAcronyms)

(=>
  (and
    (instance ?WEEK Week)
    (instance ?VISITORS Collection)
    (instance ?PURCHASES Collection)
    (instance ?PAGES Collection)
    (instance ?TOTAL_GMB CurrencyMeasure)
    (forall (?VISITOR ?ACCESSING)
      (<=>
        (and
          (instance ?VISITOR EbayVisitor)
          (member ?PAGE ?PAGES)
          (instance ?ACCESSING AccessingWebPage)
          (agent ?ACCESSING ?VISITOR)
          (destination ?ACCESSING ?PAGE)
          (during (WhenFn ?ACCESSING) ?WEEK))
        (member ?VISITOR ?VISITORS)))
    (forall (?PURCHASE ?BUYING)
      (<=>
        (and
          (instance ?BUYING Buying)
          (member ?PAGE ?PAGES)
          (instrument ?BUYING ?PAGE)
          (during (WhenFn ?BUYING) ?WEEK))
        (member ?PURCHASE ?PURCHASES))))
  (equal 
    (GPWFn ?WEEK ?PAGES)
    (DivisionFn
      (GMBFn ?PURCHASES)
      (CardinalityFn ?VISITORS))))


;; QualifyingEventsFn (Qualifying Factor/Event)
;; -------------------------------------------------------------------

(documentation QualifyingEventsFn EnglishLanguage "(QualigyingEventsFn ?EXPERIMENT) returns the 
 &%Collection of all &%Processes that are &%experimentalControlProccesses or 
 &%experimentalVariableProcesses of ?EXPERIMENT.")
    
(subclass QualifyingEventsFn UnaryFunction)

(domain QualifyingEventsFn 1 Experimenting)

(range QualifyingEventsFn Collection)

(termFormat EnglishLanguage QualifyingEventsFn "eBay Qualifying Factor or Event")

(synonymousExternalConcept "Qualifying Factor/Event" QualifyingEventsFn EbayAcronyms)

(=>
  (and
    (instance ?EXPERIMENT Experimenting)
    (instance ?QUALIFYING_EVENTS Collection)
    (forall (?EVENT)
      (<=>
        (and
          (instance ?EVENT Process)
          (or
            (experimentalControlProccess ?EXPERIMENT ?EVENT )
            (experiementalVariableProccess ?EXPERIMENT ?EVENT)))
        (member ?EVENT ?QUALIFYING_EVENTS))))
  (equal
    (QualifyingEventsFn ?EXPERIMENT)
    (?QUALIFYINGEVENTS)))

;; treatedPage
;; -------------------------------------------------------------------

(domain treatedPage 1 WebPage)

(domain treatedPage 2 Experimenting)

(termFormat EnglishLanguage treatedPage "eBay treated page")

(instance treatedPage BinaryPredicate)

(documentation treatedPage EnglishLanguage "Pages on the site where the qualified event/factor 
surface.")

(synonymousExternalConcept "Treated Page" treatedPage EbayAcronyms)

(=>
  (treatedPage ?WEBPAGE ?EXPERIMENT)
  (exists (?EVENT)
    (and
      (member ?EVENT (QualifyingEventsFn ?EXPERIMENT))
      (=>
        (exists (?VISITOR ?ACCESSING)
          (and
            (instance ?VISITOR EbayVisitor)
            (instance ?ACCESSING AccessingWebPage)
            (agent ?ACCESSING ?VISITOR)
            (destination ?ACCESSING ?WEBPAGE)))
        (experiencer ?VISITOR ?EVENT)))))

;; treatedUser
;; -------------------------------------------------------------------

(domain treatedUser 1 EbayVisitor)

(domain treatedUser 2 Experimenting)

(synonymousExternalConcept "Treated User" treatedUser EbayAcronyms)

(documentation treatedUser EnglishLanguage "A GUID/UID qualified for the experiment. (treatedUser
 ?VISITOR ?EXPERIMENT) means that ?VISITOR experienced a Qualifying Event of ?EXPERIMENT.")

(instance treatedUser BinaryPredicate)

(=>
  (and
    (treatedUser ?VISITOR ?EXPERIMENT)
    (instance ?VISITOR EbayVisitor)
    (instance ?EXPERIMENT Experimenting))
  (exists (?VISITOR ?EVENT)
    (and
      (member ?EVENT (QualifyingEventsFn ?EXPERIMENT))
      (experiencer ?VISITOR ?EVENT))))

;; TreatedUsersFn
;; -------------------------------------------------------------------

(domain TreatedUsersFn 1 Experimenting)

(domain TreatedUsersFn 2 Interval)

(range TreatedUsersFn Collection)

(synonymousExternalConcept "Treated Users" treatedUser EbayAcronyms)

(documentation TreatedUsersFn EnglishLanguage "(TreatedUsersFn ?EXPERIMENT ?INTERVAL) returns a 
 &%Collection of all &%EbayVisitors who where &%treatedUsers in ?EXPERIMENT.")


(instance TreatedUsersFn BinaryFunction)

(=>
  (and
    (instance ?EXPERIMENT Experiment)
    (instance ?INTERVAL TimeInterval))
  (exists (?TREATED_USERS)
    (and
      (instance ?TREATED_USERS Collection)
      (forall (?USER)
        (<=>
          (and
            (instance ?USER EbayVisitor)
            (treatedUser ?USER ?EXPERIMENT))
          (member ?USER ?TREATED_USERS)))
      (equal
        (TreatedUsersFn ?EXPERIMENT ?INTERVAL) ?TREATED_USERS))))


;; PPSFn (PPS)
;; -------------------------------------------------------------------

(domain PPSFn 1 Experimenting)

(domain PPSFn 2 Week)

(documentation PPSFn EnglishLanguage "Session initiated purchases per visitor per week. Accounts
 for purchases immediately following a qualifying event")

(termFormat EnglishLanguage PPSFn "eBay PPS")

(synonymousExternalConcept "PPS" PPSFn EbayAcronyms)


(=>
  (and
    (instance ?EXPERIMENT Experimenting)
    (instance ?WEEK Week))
  (equal
    (DivisionFn
      (CardinalityFn (QualifyingPurchasesFn ?EXPERIMENT ?WEEK))
      (CardinalityFn (treatedUserFn ?EXPERIMENT ?WEEK)))
    (PPSFn ?EXPERIMENT ?WEEK))

;; JW: Needs to be able to be limited limit to a test variant


;; GPSFn
;; -------------------------------------------------------------------

(domain GPSFn 1 Experimenting)

(domain GPSFn 2 TimeInterval)

(documentation GPSFn EnglishLanguage "GMB value for PPS purchases (item Price * Quantitiy 
Sold)")

(termFormat EnglishLanguage GPSFn "GPS")

(synonymousExternalConcept "GPS" GPSFn EbayAcronyms)

(=>
  (and
    (instance ?EXPERIMENT Experimenting)
    (instance ?INTERVAL TimeInterval))
  (equal 
    (DivisionFn
      (monetaryValue (KappaFn ?AMOUNT
        (and
          (member (QualifyingPurchasesFn ?EXPERIMENT ?INTERVAL))
          (transactionAmount ?SALE ?AMOUNT))))
      (CardinalityFn (treatedUserFn ?EXPERIMENT ?INTERVAL)))
    (GPSFn ?EXPERIMENT ?INTERVAL)))

;; EbayQuery (Query)
;; -------------------------------------------------------------------

(subclass Query SymbolicString)

(documentation EbayQuery EnglishLanguage "A search query is a query that a user enters into web 
 search engine to satisfy his or her information needs")

(termFormat EnglishLanguage EbayQuery "query")

(synonymousExternalConcept "Query" GPSFn EbayAcronyms)


(=>
  (instance ?QUERY Query)
  (exists (?QUESTION ?SEARCHENGINE)
    (and
      (instance ?QUESTION Questioning)
      (result ?QUESTION ?QUERY)
      (patient ?QUESTION ?SEARCHENGINE)
      (instance ?SEARCHENGINE SearchEngine))))

(=>
  (and
    (instance ?QUERY Query)
    (represents ?QUERY ?PURSUING))
  (and
    (instance ?PURSUING Pursuing)
    (exists (?AGENT ?OBJ)
      (and
        (instance ?OBJ Object)
        (instance ?AGENT Agent)
        (patient ?PURSUING ?OBJ)))))

