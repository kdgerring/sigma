;; EbayGUID (GUID)
;; -------------------------------------------------------------------

(subclass EbayGUID Identifier)

(documentation EbayGUID EnglishLanguage "Global user identifier, machine identifier based on browser
 cookies.")

(termFormat EnglishLanguage EbayGUID "eBay GUID")

(synonymousExternalConcept "GUID" EbayGUID EbayAcronyms)

(=>
  (instance ?GUID EbayGUID)
  (exists (?COMPUTER ?WEBPAGE ?ACCESSING)
    (and
      (instance ?WEBPAGE WebPage)
      (component ?WEBPAGE EbayWebSite)
      (instance ?ACCESSING AccessingWebPage)
      (instrument ?ACCESSING ?COMPUTER)
      (destination ?ACCESSING ?WEBPAGE)
      (represents ?GUID ?COMPUTER))))


;; EbayUID (UID)
;; -------------------------------------------------------------------

(subclass EbayUID Identifier)

(documentation EbayUID EnglishLanguage "User identifier, established when the user registers on
 eBay.com")

(termFormat EnglishLanguage EbayUID "eBay UID")

(synonymousExternalConcept "UID" EbayUID EbayAcronyms)

(=>
  (instance ?UID EbayUID)
  (exists (?USER)
    (and
      (instance ?USER EbayVisitor)
      (attribute ?USER RegisteredUser)
      (represents ?UID ?USER))))

;; GMBFn (GMB)
;; -------------------------------------------------------------------

(subclass GMBFn UnaryFn)

(domain GMBFn 1 Collection)

(range GMBFn CurrencyMeasure)

(documentation GMBFn EnglishLanguage "OLD: The total value of goods (in Local Currency or US
 dollars) bought by a buyer, based on The buyer country. NEW: The total value of goods in a
 collection of &%BUYINGS.")

(termFormat EnglishLanguage GMBFn "eBay GMB")

(synonymousExternalConcept "GMB" GMBFn EbayAcronyms)

(=>
  (and
    (instance ?PURCHASES Collection)
    (forall (?BUYING)
      (=>
        (member ?BUYING ?PURCHASES)
        (instance ?BUYING Buying))))
  (and
    (monetaryValue (KappaFn ?AMOUNT
      (and
        (instance ?PURCHASE Buying)
        (member ?PURCHASE ?PURCHASES)
        (transactionAmount ?PURCHASE ?AMOUNT)))
      ?TOTAL_PURCHASE_AMOUNT)
    (equal 
      (GMBFn ?PURCHASES) ?TOTAL_PURCHASE_AMOUNT)))

;; does this work? i.e. adds up all puchase amounts?

;; EbayGMVFn (GMV)
;; -------------------------------------------------------------------

(subclass EbayGMVFn BinaryFn)

(domain EbayGMVFn 1 Collection)

(domain EbayGMVFn 2 GeopoliticalArea)

(range EbayGMVFn CurrencyMeasure)

(documentation EbayGMVFn EnglishLanguage "OLD: Total amount of all purchases made  (in local 
 currency or US dollars), reported by seller country of residence (Item Price * Quantity Sold). NEW:
 (GMVFn ?COLLECTION ?REGION) is a &%BinaryFunction that returns the &%GMBFn of a &%subCollection of 
 ?COLLECTION which is limited to instances of &%Selling where the &%Agent of the &%Selling is 
 &%located in ?REGION")

(termFormat EnglishLanguage EbayGMVFn "eBay GMV")

(synonymousExternalConcept "GMV" EbayGMVFn EbayAcronyms)

(=>
  (and
    (instance ?SALES Collection)
    (instance ?REGION GeopoliticalArea)
    (forall (?SALE)
      (=>
        (member ?SALE ?SALES)
        (instance ?SALE Selling))))
  (and
    (exists (?SALES_IN_REGION)
      (and
        (instance ?SALES_IN_REGION Collection)
        (subCollection ?SALES_IN_REGION ?SALES)
        (forall (?SALE ?AGENT)
          (=>
            (and
              (agent ?SALE ?AGENT)
              (instance ?AGENT Human)
              (located ?AGENT ?REGION)
            (member ?SALE ?SALES_IN_REGION))))))
    (equal
      (GMVFn ?SALES ?REGION)
      (GMBFn ?SALES_IN_REGION))))


