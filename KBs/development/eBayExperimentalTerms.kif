;; EbayVisitor (Visitor)
;; -------------------------------------------------------------------

(subclass EbayVisitor Human)

(documentation EbayVisitor EnglishLanguage "Someone who visits the eBay site, who may or may not log
 into the site.")

(termFormat EnglishLanguage EbayVisitor "eBay Visitor")

(synonymousExternalConcept "visitor" EbayVisitor EbayAcronyms)

(=>
  (instance ?VISITOR EbayVisitor)
  (exists (?WEBPAGE ?ACCESSING)
    (and
      (instance ?WEBPAGE WebPage)
      (component ?WEBPAGE EbayWebSite)
      (instance ?ACCESSING AccessingWebPage)
      (agent ?ACCESSING ?VISITOR)
      (destination ?ACCESSING ?WEBPAGE))))
    
