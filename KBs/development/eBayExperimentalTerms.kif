      (and
        (instance ?EXPERIMENTING Experimenting)
        (instance ?EVENT Process)
        (member ?EVENT (QualifyingEventsFn ?EXPERIMENT)
        (capability ?EVENT experiencer ?Visitor))))
    (QualifiedTreatmentsFn ?VISITOR)))

