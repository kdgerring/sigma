;; HoleOpening
;; -------------

(subclass HoleOpening Region)

(documentation HoleOpening EnglishLanguage "The &%part of a &%Hole that is just the opening, as opposed to the entire negative space that composes the &%Hole. If an object passing into or out of a &%Hole, then it must pass through the HoleOpening or the HoleHost")

(termFormat EnglishLanguage HoleOpening "hole opening")

(=>
    (instance ?OPENING HoleOpening)
    (part ?OPENING ?HOLE))

(=>
    (and
        (hole ?HOLE ?HOLEHOST)
        (instance ?OBJ Object)
        (instance ?MOTION Translocation)
        (patient ?MOTION ?OBJ) 
        (path ?MOTION ?PATH)
        (part ?INSIDE_PATH ?PATH)
        (contains ?HOLE ?INSIDE_PATH)
        (part ?OUTSIDE_PATH ?PATH)
        (not
            (contains ?HOLE ?OUTSIDE_PATH)))
    (exists (?HOLE_ENTRANCE) 
        (or
            (and
                (instance ?HOLE_ENTRANCE HoleOpening)
                (part ?HOLE_ENTRANCE ?HOLE))
            (and
                (instance ?HOLE_ENTRANCE Object)
                (part ?HOLE_ENTRANCE ?HOLEHOST)))))
            


