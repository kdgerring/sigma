;; synonymousExternalConceptFunction
;; ---------------------------------

(documentation synonymousExternalConceptFunction EnglishLanguage "(synonymousExternalConceptFunction ?STRING ?FORMULA ?LANGUAGE) means that that the SUMO formula ?FORMULA has the same meaning as ?STRING in ?LANGUAGE")

(domain synonymousExternalConceptFunction 1 SymbolicString)
(domain synonymousExternalConceptFunction 2 Formula )
(domain synonymousExternalConceptFunction 3 Language )

(termFormat EnglishLanguage synonymousExternalConceptFunction "synonymous external concept function")


;; physicalCopy
;; -------------

(subclass physicalCopy BinaryRelation)

(domain physicalCopy 1 CorpuscularObject)
(domain physicalCopy 2 CorpuscularObject)

(relatedInternalConcept physicalCopy copy)

(termFormat EnglishLanguage physicalCopy "physical copy")

(documentation physicalCopy EnglishLanguage "A &%BinaryRelation where one &%CorpuscularObject is &%Likely to contain the physical characteristics of another &%CorpuscularObject. (physicalCopy ?OBJ1 ?OBJ2) means that ?OBJ1 is a &%physicalCopy of ?OBJ2")

(=>
    (and
        (physicalCopy ?OBJECT1 ?OBJECT2)
        (attribute ?OBJECT2 ?ATTRIBUTE))
    (modalAttribute 
        (attribute ?OBJECT1 ?ATTRIBUTE) Likely))

;; JW: This definition needs expansion. 
;; We are only concerned with physical characteristics, 
;; so there are some attributes which should be immediately excluded.


;; Antique
;; Breadcrumb: Antiques (20081)
;; ----------

(instance Antique Attribute)

(documentation Antique EnglishLanguage "An &%Artifact that is older than 30 years, and worth more than newer items of the same class")

(termFormat EnglishLanguage Antique "antique")

(synonymousExternalConcept "20081" Antique EbayCategories)



(=>
    (attribute ?ANTIQUE Antique)
    (exists (?AGE) 
        (and
            (age ?ANTIQUE (MeasureFn ?AGE YearDuration))
            (greaterThan ?AGE 30))))


(=>
    (and
        (instance ?ANTIQUE ?ARTIFACT_CLASS)
        (instance ?NEW_ITEM ?ARTIFACT_CLASS)
        (attribute ?ANTIQUE Antique)
        (not
            (attribute ?NEW_ITEM Antique)))
    (exists (?ANTIQUE_VALUE ?NEW_VALUE)
        (and
            (monetaryValue ?ANTIQUE ?ANTIQUE_VALUE)
            (monetaryValue ?NEW_ITEM ?NEW_VALUE)
            (greaterThan ?ANTIQUE_VALUE ?NEW_VALUE))))
                


