(synonymousExternalConceptFormula "63520"
  (KappaFn ?X  
    (and
      (attribute ?X Antique)
      (or
        (instance ?X Window)
        (hasPurpose ?X
          (exists (?Y ?T)
            (and
              (instance ?Y Window)
              (holdsDuring ?T
                (supports ?X ?Y)))))
        (and
          (instance ?X Lock)
          (hasPurpose ?X
            (exists (?Y ?OPENING ?CLOSING)
              (and
                (instance ?Y Window)
                (instance ?OPENING Opening)
                (instance ?CLOSING Closing)
                (instrument ?OPENING ?X)
                (instrument ?CLOSING ?X)
                (patient ?OPENING ?Y)
                (patient ?CLOSING ?Y))))))))
  EbayCategories)

