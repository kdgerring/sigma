;; Decorating
;; ---------------

(subclass Decorating Attaching)

(documentation Decorating EnglishLangauge "&%Attaching of two objects, where a &%Human &%prefers the &%result of the &%Attaching (i.e. the &%MerologicalSumFn), and the &%result of the &%Attaching does not add any additional &%capability except &%CausingHappiness.")

(termFormat EnglishLanguage Decorating "decorating")

(=>
    (instance ?DECORATING Decorating)
    (hasPurpose ?DECORATING
        (exists (?OBJ1 ?OBJ2 ?PERSON)
            (and
                (instance ?PERSON Human)
                (patient ?DECORATING ?OBJ1)
                (patient ?DECORATING ?OBJ2)
                (prefers ?PERSON
                    (exists (?OBJ3)
                        (result ?DECORATING ?OBJ3))
                    (not
                        (exists (?OBJ3)
                            (result ?DECORATING ?OBJ3))))))))

(=>
    (instance ?DECORATING Decorating)
    (hasPurpose ?DECORATING
            (exists (?OBJ ?BEFORE ?AFTER)
                (and
                    (patient ?DECORATING ?OBJ)
                    (before ?BEFORE (WhenFn ?DECORATING))
                    (before (WhenFn ?DECORATING) ?AFTER)
                    (forall (?PROCESS ?CASEROLE)
                        (=>
                            (holdsDuring ?AFTER
                                (capability ?PROCESS ?CASEROLE ?OBJ))
                            (holdsDuring ?BEFORE
                                (capability ?PROCESS ?CASEROLE ?OBJ))))))))

(=>
    (instance ?DECORATING Decorating)
    (hasPurpose ?DECORATING
            (exists (?OBJ1 ?OBJ2 ?OBJ3 ?BEFORE ?AFTER)
                (and
                    (patient ?DECORATING ?OBJ1)
                    (patient ?DECORATING ?OBJ2)
                    (before ?BEFORE (WhenFn ?DECORATING))
                    (before (WhenFn ?DECORATING) ?AFTER)
                    (result ?DECORATING ?OBJ3)
                    (forall (?PROCESS ?CASEROLE)
                        (=>
                            (holdsDuring ?AFTER
                                (capability ?PROCESS ?CASEROLE ?OBJ3))
                            (or
                                (holdsDuring ?BEFORE
                                    (capability ?PROCESS ?CASEROLE ?OBJ1))
                                (holdsDuring ?BEFORE
                                    (capability ?PROCESS ?CASEROLE ?OBJ2))
                                (equal ?PROCESS CausingHappiness))))))))



;; IntentionalDecoration
;; ---------------------

(subclass IntentionalDecoration Artifact)

(documentation IntentionalDecoration EnglishLanguage "Something that &%hasPurpose of being the &%patient in the process of &%Decorating")

(termFormat EnglishLanguage IntentionalDecoration "intentional decoration")

(=> 
    (instance ?DECORATION IntentionalDecoration)
    (hasPurpose ?DECORATION
        (exists (?PROCESS)
            (and
                (instance ?PROCESS Decorating)
                (patient ?PROCESS ?DECORATION)))))


;; Decorative
;; --------------------

(subclass Decorative RelationalAttribute)

(documentation Decorative EnglishLanguage "Something that is used in the process of &%Decorating, and was not at the location until decoration occurs.")

(termFormat EnglishLanguage Decorative "decorative")

(=>
    (attribute ?OBJ Decorative)
    (exists (?DECORATING ?LOC ?BEFORE)
        (and 
            (instance ?DECORATING Decorating)
            (patient ?DECORATING ?OBJ)
            (eventLocated ?DECORATING ?LOC)
            (before ?BEFORE (WhenFn ?DECORATING))
            (not
                (holdsDuring ?BEFORE
                    (located ?OBJ ?LOC))))))

;; JW: This is a start, but is not sufficient. 
;; e.g. includes the hook that supports the actual decoration.

