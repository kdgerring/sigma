;; JW: This mapping needs refinement. A reproduction may not be a &%physicalCopy of an existing 
;; Antiquity, but also simply made to look generally like one.


;; Antiques (20081) > Antiquities (37903) > Price Guides & Publications (171169)
;; -------------------------------------------------------------------

(synonymousExternalConceptFormula "171169"
  (KappaFn ?X 
    (and 
      (instance ?X FactualText)
      (exists (?ANTIQUITY)
        (and
          (instance ?ANTIQUITY Antiquity)
          (refers ?X ?ANTIQUITY)))))
  EbayCategories)


;; Antiques (20081) > Antiquities (37903) > Other (73464)
;; -------------------------------------------------------------------

(synonymousExternalConceptFormula "73464"
  (KappaFn ?X
    (and
      (instance ?X Antiquity)
      (instance ?MAKING Making)
      (result ?MAKING ?X)
      (not 
        (or
          (attribute ?X (MadeInFn  NorthAmerica))
          (attribute ?X (MadeInFn SouthAmerica))
          (attribute ?X (MadeInFn  MiddleAmerica))))
      (not 
        (attribute ?X (MadeInFn  ByzantineEmpire)))
      (not 
        (attribute ?X (MadeInFn CelticNation)))
      (not 
        (attribute ?X (MadeInFn AncientEgypt)))
      (not 
        (or
          (attribute ?X (MadeInFn EasternAsia))
          (attribute ?X (MadeInFn NorthernAsia))))
      (not 
        (attribute ?X (MadeInFn AncientGreece)))
      (not 
        (or
          (attribute ?X (MadeInFn Israel))
          (attribute ?X (MadeInFn WestBank))))
      (not
        (exists (?NATION ?N)
          (and
            (instance ?NATION Nation)
            (eventLocated ?MAKING ?NATION)
            (beliefGroupPercentInRegion Islam ?N ?NATION)
            (greaterThan ?N 50))))
      (not
        (or
          (attribute ?X (MadeInFn MiddleEastRegion))
          (attribute ?X (MadeInFn Turkey))))
      (not 
        (before (EndFn (WhenFn ?MAKING)) (EndFn NeolithicPeriod)))
      (not 
        (attribute ?X (MadeInFn AncientRome)))
      (not 
        (attribute ?X (MadeInFn SouthernItaly)))
      (not 
        (attribute ?X (MadeInFn VikingNation)))))
  EbayCategories)


;; Antiques (20081) > Architectural & Garden (4707)
;; -------------------------------------------------------------------

(synonymousExternalConceptFormula "4707"
  (KappaFn ?X 
    (and
      (hasPurpose ?ITEM
        (exists (?LOCATION) 
          (and
            (or
              (instance ?LOCATION CultivatedLandArea)
              (instance ?LOCATION StationaryArtifact))
            (or 
              (part ?ITEM ?LOCATION)
              (exists (?PROCESS)
                (and
                  (instance ?PROCESS IntentionalProcess)
                  (patient ?PROCESS ?ITEM)
                  (destination ?PROCESS ?LOCATION)))))))
      (attribute ?X Antique))) 
  EbayCategories)

;; JW: This definition needs further work. 
;; Does not preclude a ?PROCESS such as someone putting a random item in ?LOCATION, e.g. littering.


