;; Antiques (20081) > Antiquities (37903) > Islamic (162918)
;; -------------------------------------------------------------------

(synonymousExternalConceptFormula "162918"
  (KappaFn ?X
    (and
      (instance ?X Antiquity)
      (exists (?MAKING ?NATION ?N) 
        (and
          (instance ?MAKING Making)
          (result ?MAKING ?X)
          (eventLocated ?MAKING ?NATION)
          (beliefGroupPercentInRegion Islam ?N ?NATION)
          (greaterThan ?N 50)))))
  EbayCategories)


