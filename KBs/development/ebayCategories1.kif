;; -------------------------------------------------

(subclass Antiquity Artifact)

(documentation Antiquity EnglishLanguage "From Wikipedia: An &%Artifact created before the &%MiddleAges.")

(termFormat EnglishLanguage Antiquity "antiquity")

(synonymousExternalConcept "37903" Antiquity EbayCategories)

(=> 
    (instance ?ITEM Antiquity)
    (exists (?MAKING) 
        (and
            (instance ?MAKING Making)
            (result ?MAKING ?ITEM)
            (before (EndFn (WhenFn ?MAKING)) (BeginFn MiddleAges)))))

