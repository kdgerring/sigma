(synonymousExternalConceptFormula "91101"
  (KappaFn ?X
    (and
      (instance ?X Antiquity)
    (or
      (attribute ?X (MadeInFn MiddleEastRegion))
      (attribute ?X (MadeInFn Turkey)))))
  EbayCategories)


;; Antiques (20081) > Antiquities (37903) > Neolithic & Paleolithic (66834)
;; -------------------------------------------------------------------

(synonymousExternalConceptFormula "66834"
  (KappaFn ?X
    (and
      (instance ?X Antiquity)
      (exists (?MAKING) 
        (and
          (instance ?MAKING Making)
          (result ?MAKING ?X)
          (before (BeginFn PaleolithicPeriod) (BeginFn (WhenFn ?MAKING))
          (before (EndFn (WhenFn ?MAKING)) (EndFn NeolithicPeriod))))))) 
  EbayCategories)


