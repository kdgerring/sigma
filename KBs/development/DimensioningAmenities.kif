(=>
	(defaultMinimumHeight ?O ?H)
	(modalAttribute
		(exists (?INST)
			(and
				(instance ?INST ?O)
				(height ?INST ?HEIGHT)
				(lessThan ?HEIGHT ?H))) Unlikely))
				
(=>
	(and
		(subclass ?S ?O)
		(defaultMinimumHeight ?O ?H))
	(defaultMinimumHeight ?S ?H))	
		
(instance defaultMinimumHeight BinaryPredicate)
(domainSubclass defaultMinimumHeight 1 Object)
(domainSubclass defaultMinimumHeight 2 Quantity)
(documentation defaultMinimumHeight EnglishLanguage "A &%BinaryPredicate that states the assumed minimum &%height of an &%Object")

(=>
	(defaultMaximumHeight ?O ?H)
	(modalAttribute
		(exists (?INST)
			(and
				(instance ?INST ?O)
				(height ?INST ?HEIGHT)
				(greaterThan ?HEIGHT ?H))) Unlikely))
				
(=>
	(and
		(subclass ?S ?O)
		(defaultMaximumHeight ?O ?H))
	(defaultMaximumHeight ?S ?H))
		
