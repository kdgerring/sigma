;;There are two parts to a baby monitoring system - reciever and transmitter. If there is a system with reciever ?RX and transmitter ?TX, then their
;; respective measurements are...
(=> 
  (and
    (instance ?X BabyMonitoringSystem)
    (instance ?RX RadioReciever)
    (instance ?TX Device)
    (engineeringSubcomponent ?TX ?X)
    (engineeringSubcomponent ?RX ?X))
	(and
		(defaultMinimumHeight ?RX (MeasureFn 2 Centimeter)) 
		(defaultMaximumHeight ?RX (MeasureFn 6 Centimeter))
		(defaultMinimumWidth ?RX (MeasureFn 2 Inch))
		(defaultMaximumWidth ?RX (MeasureFn 4 Inch))
		(defaultMinimumLength ?RX (MeasureFn 3 Inch))
		(defaultMaximumLength ?RX (MeasureFn 6 Inch))
		(defaultMinimumMeasure ?RX (MeasureFn 8 Ounce))
		(defaultMaximumMeasure ?RX (MeasureFn 24 Ounce))
		(defaultMinimumHeight ?TX (MeasureFn 2 Centimeter))
