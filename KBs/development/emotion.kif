(subclass MovingTowardsPeople EmotionalBodilyMovementBehavior)
(subclass MovingTowardsPeople Translocation)

(=>
  (and
    (instance ?MTP MovingTowardsPeople)
    (origin ?MTP ?O)
    (destination ?MTP ?D))
  (not
    (equal ?O ?D)))
    
(=>
  (and
    (instance ?MTP MovingTowardsPeople)
    (agent ?MTP ?A))
  (exists (?P)
    (and
      (instance ?P Human)
      (holdsDuring 
        (ImmediatePastFn 
          (WhenFn ?MTP))
        (distance ?MTP ?A ?D1))
      (holdsDuring 
        (ImmediateFutureFn 
          (WhenFn ?MTP))
        (distance ?MTP ?A ?D2))
      (greaterThan ?D1 ?D2))))
      
