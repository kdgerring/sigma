(relatedInternalConcept DisgustedVoiceUtterance Disgust)
(termFormat EnglishLanguage DisgustedVoiceUtterance "canonical disgusted voice utterance")

(=>
  (and
    (instance ?DVU DisgustedVoiceUtterance)
    (agent ?DVU ?A))
  (exists (?D)
    (and
      (instance ?D Disgust)
      (experiencer ?D ?A)
      (causes ?D ?DVU)))) 
      
