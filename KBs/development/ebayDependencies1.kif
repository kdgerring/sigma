    (path ?MOTION ?PATH)
    (part ?KEYHOLE ?PATH))
  (part ?ESC_HOLE ?PATH))


;; CeilingBeam
;; -------------------------------------------------------------------

(subclass CeilingBeam BoardOrBlock)

(documentation CeilingBeam EnglishLanguage "A &%BoardOrBlock that &%hasPurpose of keeping a
 &%Ceiling from &%Falling, and its &%length is greater than the &%length or &%width of a &%Room.")

(termFormat EnglishLanguage CeilingBeam "ceiling beam")

(=>
  (instance ?BEAM CeilingBeam)
  (hasPurpose ?BEAM
    (exists (?CEILING ?T)
      (and
        (instance ?CEILING Ceiling)
        (holdsDuring ?T
          (and
            (orientation ?CEILING ?BEAM On)
            (not
              (exists (?FALLING)
                (and
                  (instance ?FALLING Falling)
                  (patient ?FALLING ?CEILING))))))))))

(=>
  (and
    (instance ?BEAM CeilingBeam)
    (length ?BEAM ?BEAMLENGTH))
  (hasPurpose ?BEAM
    (exists (?ROOM ?ROOMLENGTH ?ROOMWIDTH)
      (and
        (instance ?ROOM Room)
        (length ?ROOM ?ROOMLENGTH)
        (width ?ROOM ?ROOMWIDTH)
        (or
          (greaterThan (MeasureFn ?BEAMLENGTH) (MeasureFn ?ROOMLENGTH))
          (greaterThan (MeasureFn ?BEAMLENGTH) (MeasureFn ?ROOMWIDTH)))))))

(=>
  (instance ?BEAM CeilingBeam)
  (attribute ?BEAM LongAndThin))

