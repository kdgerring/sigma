;; Antique
;; -------------------------------------------------------------------

(instance Antique Attribute)

(documentation Antique EnglishLanguage "An &%Artifact that is older than 30 years, and worth more 
  than newer items of the same class")

(termFormat EnglishLanguage Antique "antique")

(=>
  (attribute ?ANTIQUE Antique)
  (exists (?AGE) 
    (and
      (age ?ANTIQUE (MeasureFn ?AGE YearDuration))
      (greaterThan ?AGE 30))))


(=>
  (and
    (instance ?ANTIQUE ?ARTIFACT_CLASS)
    (instance ?NEW_ITEM ?ARTIFACT_CLASS)
    (attribute ?ANTIQUE Antique)
    (not
      (attribute ?NEW_ITEM Antique)))
  (exists (?ANTIQUE_VALUE ?NEW_VALUE)
    (and
      (monetaryValue ?ANTIQUE ?ANTIQUE_VALUE)
      (monetaryValue ?NEW_ITEM ?NEW_VALUE)
      (greaterThan ?ANTIQUE_VALUE ?NEW_VALUE))))
        
