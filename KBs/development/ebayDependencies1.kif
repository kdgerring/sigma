;; Decorative
;; -------------------------------------------------------------------

(subclass Decorative RelationalAttribute)

(documentation Decorative EnglishLanguage "Something that is used in the process of &%Decorating, 
  and was not at the location until decoration occurs.")

(termFormat EnglishLanguage Decorative "decorative")

(=>
  (attribute ?OBJ Decorative)
  (exists (?DECORATING ?LOC ?BEFORE)
    (and 
      (instance ?DECORATING Decorating)
      (patient ?DECORATING ?OBJ)
      (eventLocated ?DECORATING ?LOC)
      (before ?BEFORE (WhenFn ?DECORATING))
      (not
        (holdsDuring ?BEFORE
          (located ?OBJ ?LOC))))))

