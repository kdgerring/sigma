;; physicalCopy
;; -------------------------------------------------------------------

(subclass physicalCopy BinaryRelation)

(domain physicalCopy 1 CorpuscularObject)
(domain physicalCopy 2 CorpuscularObject)

(relatedInternalConcept physicalCopy copy)

(termFormat EnglishLanguage physicalCopy "physical copy")

(documentation physicalCopy EnglishLanguage "A &%BinaryRelation where one &%CorpuscularObject is 
  &%Likely to contain the physical characteristics of another &%CorpuscularObject. (physicalCopy 
  ?OBJ1 ?OBJ2) means that ?OBJ1 is a &%physicalCopy of ?OBJ2")

(=>
  (and
    (physicalCopy ?OBJECT1 ?OBJECT2)
    (attribute ?OBJECT2 ?ATTRIBUTE))
  (modalAttribute 
    (attribute ?OBJECT1 ?ATTRIBUTE) Likely))

