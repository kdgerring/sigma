;; RotationallySymmetric
;; -------------------------------------------------------------------

(instance RotationallySymmetric ShapeAttribute)

(documentation RotationallySymmetric EnglishLanguage "The &%ShapeAttribute where if an object is
 rotated about an &%axis, the &%distance from the axis to the surface is the same for all lines
 &%perpendicular to the axis that share a point on the axis.")

(termFormat EnglishLanguage RotationallySymmetric "rotationally symmetric")


(=>
  (and
    (attribute ?OBJ RotationallySymmetric)
    (surface ?SURFACE ?OBJ))
  (=>
    (and    
      (instance ?ROTATING Rotating)
      (axis ?AXIS ?OBJ)
      (patient ?ROTATING ?OBJ)
      (instance ?LINE1 OneDimensionalFigure)
      (instance ?LINE2 OneDimensionalFigure)
      (perpendicular ?LINE1 ?AXIS)
      (perpendicular ?LINE2 ?AXIS)
      (instance ?AXIS_POINT GeometricPoint)
      (part ?AXIS_POINT ?AXIS)
      (instance ?SURFACE_POINT1 GeometricPoint)
      (instance ?SURFACE_POINT2 GeometricPoint)
      (part ?SURFACE_POINT1 ?SURFACE)
      (part ?SURFACE_POINT2 ?SURFACE)
      (part ?AXIS_POINT ?LINE1)
      (part ?AXIS_POINT ?LINE2)
      (part ?SURFACE_POINT1 ?LINE1)
      (part ?SURFACE_POINT2 ?LINE2)
      (distance ?AXIS_POINT ?SURFACE_POINT1 ?QUANT1)
      (distance ?AXIS_POINT ?SURFACE_POINT2 ?QUANT2))
    (equal ?QUANT1 ?QUANT2)))


;; HandRail
;; -------------------------------------------------------------------

(subclass HandRail StationaryArtifact)

(documentation Handrail EnglishLanguage "A &%StationaryArtifact meant to be &%grasped by the hand to prevent
 someone from falling, often at the side of a staircase or balcony.")

(termFormat EnglishLanguage HandRail "railing")

(=>
  (instance ?RAIL HandRail)
  (hasPurpose ?RAIL
    (exists (?HUMAN ?T)
      (and
        (instance ?HUMAN Human)
        (holdsDuring ?T
          (and
            (grasps ?HUMAN ?RAIL)
            (not
                (exists (?FALLING)
                      (and
