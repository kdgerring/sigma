;; HeatingVent
;; -------------------------------------------------------------------

(subclass HeatingVent Hole)

(documentation HeatingVent EnglishLanguage "A &%Hole in a &%Wall, &%Floor, or &%Celing that
 &%hasPurpose of being an path of &%Heating.")

(termFormat EnglishLanguage HeatingVent "heating vent")

(=>
  (instance ?VENT HeatingVent)
  (exists (?SURFACE)
    (and
      (equal ?SURFACE
        (HoleHostFn ?VENT))
      (or
        (instance ?SURFACE Wall)
        (instance ?SURFACE Ceiling)
        (instance ?SURFACE Floor)))))

(=>
  (instance ?VENT HeatingVent)
  (hasPurpose ?VENT
    (exists (?HEATING ?AIR ?GM ?ROOM ?GAS_PATH)
      (and
        (instance ?HEATING Heating)
        (instance ?AIR Air)
        (instance ?GM GasMotion)
        (instance ?ROOM Room)
        (patient ?GM ?AIR)
        (patient ?HEATING ?ROOM)
        (subProcess ?GM ?HEATING)
        (path ?GM ?GAS_PATH)
        (beforeOnPath ?VENT ?ROOM ?GAS_PATH ?GM)))))


