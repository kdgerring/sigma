(note TQG48)

;; Row variable expansion.

(instance testPred48 QuaternaryPredicate)
(domain testPred48 1 Entity)
(domain testPred48 2 Entity)
(domain testPred48 3 Entity)
(domain testPred48 4 Entity)

(instance Entity48-1 Entity)
(instance Entity48-2 Entity)
(instance Entity48-3 Entity)
(instance Entity48-4 Entity)

(=>
  (testPred48 ?ITEM1 ?ITEM2 @ROW)
  (and
    (instance ?ITEM1 Amphibian)
    (instance ?ITEM2 Reptile)))

(testPred48 Entity48-1 Entity48-2 Entity48-3 Entity48-4)
(testPred48 Entity48-2 Entity48-1 Entity48-3 Entity48-4)

(query (instance ?X Reptile))

(answer (?X Entity48-1))
(answer (?X Entity48-2))
