(note TQG49)

;; Row variable expansion.

(instance testPred49 QuaternaryPredicate)
(domain testPred49 1 Entity)
(domain testPred49 2 Entity)
(domain testPred49 3 Entity)
(domain testPred49 4 Entity)

(instance Entity49-1 Entity)
(instance Entity49-2 Entity)
(instance Entity49-3 Entity)
(instance Entity49-4 Entity)

(=>
  (testPred49 @ROW ?ITEM1 ?ITEM2)
  (and
    (instance ?ITEM1 Amphibian)
    (instance ?ITEM2 Reptile)))

(testPred49 Entity49-1 Entity49-2 Entity49-3 Entity49-4)
(testPred49 Entity49-1 Entity49-2 Entity49-4 Entity49-3)

(query (instance ?X Reptile))

(answer (?X Entity49-4))
(answer (?X Entity49-3))
