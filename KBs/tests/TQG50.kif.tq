(note TQG50)

;; Row variable expansion.

(instance testPred50 QuaternaryPredicate)
(domain testPred50 1 Entity)
(domain testPred50 2 Entity)
(domain testPred50 3 Entity)
(domain testPred50 4 Entity)

(instance Entity50-1 Entity)
(instance Entity50-2 Entity)
(instance Entity50-3 Entity)
(instance Entity50-4 Entity)

(=>
  (testPred50 @ROW ?ITEM1 ?ITEM2)
  (and
    (instance ?ITEM1 Amphibian)
    (instance ?ITEM2 Reptile)))

(testPred50 Entity50-1 Entity50-2 Entity50-3 Entity50-4)
(testPred50 Entity50-1 Entity50-2 Entity50-4 Entity50-3)

(query (instance ?X Amphibian))

(answer (?X Entity50-4))
(answer (?X Entity50-3))
