(note TQG4)

(instance Joe Human)

(agent DoingSomething Joe)

(instance DoingSomething IntentionalProcess)

(=>
  (and
    (agent ?PROC ?AGENT)
    (instance ?PROC IntentionalProcess))
  (and
    (instance ?AGENT CognitiveAgent)
    (not
      (holdsDuring
        (WhenFn ?PROC)
        (attribute ?AGENT Dead)))))

(query (not (holdsDuring (WhenFn DoingSomething) (attribute Joe Dead))))

(answer yes)