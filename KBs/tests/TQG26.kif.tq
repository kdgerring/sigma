(note TQG26)

;; case elimination.

(instance Planet Class)
(subclass Planet AstronomicalBody)

(=>
  (instance ?P Planet)
  (or
    (attribute ?P Solid)
    (attribute ?P Gaseous)))

(=>
  (instance ?P Planet)
  (or
    (attribute ?P Earthlike)
    (attribute ?P HostileToEarthLife)))

(=>
  (and
    (instance ?X Planet)
    (attribute ?X Gaseous))
  (not (attribute ?X Earthlike)))

(instance Planet26 Planet)
(not (attribute Planet26 Solid))

(query (attribute Planet26 HostileToEarthLife))

(answer yes)
