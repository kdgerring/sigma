third of ?WHOLE.")
(subrelation third part)

(=>
  (third ?T ?W)
  (exists (?O1 ?O2)
    (and
      (third ?O1 ?W)
      (third ?O2 ?W)
      (not (equal ?O1 ?T))
      (not (equal ?O2 ?T))
      (not (equal ?O1 ?O2))
      (equal ?W 
        (MereologicalSumFn ?T 
	  (MereologicalSumFn ?O1 ?O2))))))

