(documentation ITProcess "A process performed on a computer by a human
operator.")
(subclass ITProcess IntentionalProcess)
(documentation "&%Each ITProcess is performed by a human on a computer.")
(=>
	(instance ?PROCESS ITProcess)
	(exists (?AGENT ?PATIENT)
		(and
			(agent ?PROCESS ?AGENT)
			(patient ?PROCESS ?PATIENT)
			(instance ?AGENT ITAgent)
			(instance ?PATIENT Computer))))

