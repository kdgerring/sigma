		(totalFacilityTypeInArea 
			?AREA (ExtensionFn AirportWithUnpavedRunway) ?COUNT)
		(greaterThan ?COUNT 0))
	(exists (?AIRPORT ?RUNWAY)
		(and
			(instance ?AIRPORT Airport)
			(instance ?RUNWAY UnpavedRunway)
			(part ?RUNWAY ?AIRPORT)
			(located ?AIRPORT ?AREA)))))

