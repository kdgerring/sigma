	(systemLink ?NODE2 ?NODE1 ?ARC))

(=>
	(systemLink ?NODE1 ?NODE2 ?ARC)
	(connects ?ARC ?NODE1 ?NODE2))

(=>
	(connects ?ARC ?NODE1 ?NODE2)
	(connects ?ARC ?NODE2 ?NODE1))

;; FUNCTION:
